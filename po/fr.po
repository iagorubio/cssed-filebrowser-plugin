#: src/callbacks.c:741
msgid "Enter directory name"
msgstr "Saisissez le nom du répertoire"

#: src/callbacks.c:747
msgid "This file already exists"
msgstr "Ce fichier existe déjà"

#: src/callbacks.c:747
#, fuzzy, c-format
msgid ""
"This file exists on the system.\n"
"%s\n"
"\n"
"Please use other name for the new directory"
msgstr "Veuillez choisir un autre nom pour le nouveau répertoire"

#: src/callbacks.c:761
msgid "Directory creation error"
msgstr "Erreur lors de la création du répertoire"

#: src/callbacks.c:780 src/callbacks.c:808
msgid "Copy path"
msgstr "Copier le chemin"

#: src/callbacks.c:785 src/callbacks.c:813
msgid "Rename"
msgstr "Renommer"

#: src/callbacks.c:791 src/callbacks.c:818
msgid "Change permissions"
msgstr "Changer les autorisations"

#: src/callbacks.c:827
msgid "Create directory"
msgstr "Créer un répertoire"

